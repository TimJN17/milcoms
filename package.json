{
  "name": "milcoms",
  "version": "1.1.4",
  "publisher": "timjn17",
  "engines": {
    "vscode": "^1.96.3"
  },
  "displayName": "MilComs",
  "license": "BSD-3-Clause",
  "description": "This project will provide a more enjoyable commenting style through commenting and emojis similar to a military weapons system operating manual. It will use the following emojis: [ ‚úÖ‚ùóüö® ‚õî ‚ö†Ô∏è üèÜ üí•].",
  "author": {
    "name": "TimJN17"
  },
  "categories": [
    "Programming Languages",
    "Formatters",
    "Data Science",
    "Machine Learning",
    "Notebooks"
  ],
  "keywords": [
    "commenting",
    "emojis",
    "emoji",
    "miltiary",
    "documntation"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TimJN17/milcoms"
  },
  "homepage": "https://github.com/TimJN17/milcoms",
  "bugs": {
    "url": "https://github.com/TimJN17/milcoms/issues",
    "email": "tim.j.n17@gmail.com"
  },
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "pricing": "Free",
  "contributes": {
    "commands": [
      {
        "command": "extension.insertDanger",
        "title": "DANGER COMMENT"
      },
      {
        "command": "extension.insertAlert",
        "title": "ALERT COMMENT"
      },
      {
        "command": "extension.insertCaution",
        "title": "CAUTION COMMENT"
      },
      {
        "command": "extension.insertSuccess",
        "title": "SUCCESS COMMENT"
      },
      {
        "command": "extension.insertVictory",
        "title": "VICTORY COMMENT"
      },
      {
        "command": "extension.insertBoom",
        "title": "BOOM COMMENT"
      }
    ],
    "languages": [
      {
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "markdown": "github",
  "icon": "./assets/20250212-Tank-128x128.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.3",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  }
